{
  "name": "strime",
  "version": "3.4.0",
  "description": "Streaming JSON projection engine â€” extract fields at multi-gigabit speeds with O(1) memory",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "strime": "./dist/cli/index.js"
  },
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "vitest",
    "build": "tsup src/index.ts src/cli/index.ts --format esm --dts --clean --splitting --out-dir dist",
    "dev": "vitest",
    "bench": "tsx src/benchmarks/benchmark.ts",
    "bench:parallel": "tsx src/benchmarks/ndjson-parallel-benchmark.ts",
    "stress": "tsx src/benchmarks/stress.ts",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "keywords": [
    "strime",
    "json",
    "streaming",
    "projection",
    "parser",
    "ndjson",
    "high-performance",
    "zero-copy",
    "data-extraction",
    "typescript"
  ],
  "author": "Erdem Arslan <erdemarslan@ymail.com>",
  "license": "BSL-1.1",
  "devDependencies": {
    "@commitlint/cli": "^20.3.0",
    "@commitlint/config-conventional": "^20.3.0",
    "@eslint/js": "^9.39.2",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/node": "^25.0.3",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "semantic-release": "^25.0.2",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.52.0",
    "vitest": "^1.0.0"
  }
}
